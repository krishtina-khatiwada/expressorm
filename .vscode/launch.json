{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Run ts-node",
            "runtimeExecutable": "ts-node",
            "cwd": "${workspaceFolder}",
            "args": []
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Run esbuild",
            "runtimeExecutable": "esbuild",
            "cwd": "${workspaceFolder}/node_modules/@esbuild-kit/core-utils",
            "args": []
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/src/index.ts",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        }
    ]
}